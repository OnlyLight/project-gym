include ./button

.section-quality
  .container
    .section-quality__tab
      ul.nav.nav-pills
        li.nav-item
          a.section-quality__tab.nav-link.active(data-toggle='pill' href='#home') Monthly
        li.nav-item
          a.section-quality__tab.nav-link(data-toggle='pill' href='#menu1') Fixed term
        li.nav-item
          a.section-quality__tab.nav-link(data-toggle='pill' href='#menu2') Day pass
      .tab-content
        #home.tab-pane.container.active
          .section-fee
            #js-monthly.section-fee__monthly.row
              .section-fee__three.col-12.col-lg-4
                .card-fee
                  h2.card-fee__title 3 month
                  .card-fee__icon
                    i.icon.icon-24-time &#xe80d;
                    i.icon.icon-hand &#xe80c;
                    i.icon.icon-wifi &#xe80b;
                    i.icon.icon-park &#xe80a;
                    i.icon.icon-wash &#xe809;
                    i.icon.icon-weight &#xe808;
                  .card-fee__des
                    .card-fee__des__title Package of 03 months
                    .card-fee__des__item
                      .card-fee__des__text Cost
                      p.card-fee__des__number 1.900.000đ
                    .card-fee__des__item No additional joining free
                    .card-fee__des__item Convenient one time payment
                  +button('card-fee__btn', 'choose', 'fa fa-check')
              .section-fee__six.col-12.col-lg-4.mt-3.mt-lg-0
                .card-fee
                  h2.card-fee__title 6 month
                  .card-fee__icon
                    i.icon.icon-24-time &#xe80d;
                    i.icon.icon-hand &#xe80c;
                    i.icon.icon-wifi &#xe80b;
                    i.icon.icon-park &#xe80a;
                    i.icon.icon-wash &#xe809;
                    i.icon.icon-weight &#xe808;
                  .card-fee__des
                    .card-fee__des__title Package of 06 months
                    .card-fee__des__item
                      .card-fee__des__text Cost
                      p.card-fee__des__number 2.100.000đ
                    .card-fee__des__item No additional joining free
                    .card-fee__des__item Convenient one time payment
                  +button('card-fee__btn', 'choose', 'fa fa-check')
              .section-fee__twele.col-12.col-lg-4.mt-3.mt-lg-0
                .card-fee
                  h2.card-fee__title 12 month
                  .card-fee__icon
                    i.icon.icon-24-time &#xe80d;
                    i.icon.icon-hand &#xe80c;
                    i.icon.icon-wifi &#xe80b;
                    i.icon.icon-park &#xe80a;
                    i.icon.icon-wash &#xe809;
                    i.icon.icon-weight &#xe808;
                  .card-fee__des
                    .card-fee__des__title Package of 12 months
                    .card-fee__des__item
                      .card-fee__des__text Cost
                      p.card-fee__des__number 3.600.000đ
                    .card-fee__des__item No additional joining free
                    .card-fee__des__item Convenient one time payment
                  +button('card-fee__btn', 'choose', 'fa fa-check')
        #menu1.tab-pane.container.fade 2
        #menu2.tab-pane.container.fade 3
